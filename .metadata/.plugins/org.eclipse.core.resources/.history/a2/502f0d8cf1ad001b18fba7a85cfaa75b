package miniProjeAsalSayi;

public class Main {

	public static void main(String[] args) 
	{
		int number = 1;
		boolean asal = true;
		
		
		
		for(int i=2; i<number; i++)
		{
			if(number <=1)
			{
				System.out.println("Geçersiz sayý");
				asal=false;
				break;
			}
			if(number % i==0)
			{
				asal=false;
				break;
			
			}
			
			
		}
		if(asal==true){System.out.println("Asaldýr");}
		else {System.out.println("Asal deðildir");}
		
	}

}
