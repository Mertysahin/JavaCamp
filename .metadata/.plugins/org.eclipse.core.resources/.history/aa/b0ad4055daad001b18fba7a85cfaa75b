package miniProjeAsalSayi;

public class Main {

	public static void main(String[] args) 
	{
		int number = 25;
		boolean asal = false;
		
		for(int i=1; i<number; i++)
		{
			if(number % i == 1) 
			{
				asal = true;
			}
			else 
			{
				System.out.println("Asal Deðildir");
			}
		}
		if(asal==true){System.out.println("Asaldýr");}
	}

}
